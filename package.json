{
  "name": "cognito-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^4.2.3",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "mongoose": "^5.3.16",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "jest": "^25.2.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "nodemon": "^2.0.3"
  }
}
